{
    "name": "angejia/sandbox-web",
    "license": "proprietary",
    "type": "project",
    "require": {
        "laravel/framework": "5.1.*",
        "predis/predis": "^1.0",
        "barryvdh/laravel-ide-helper": "^2.1",
        "vinkla/gitlab": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "http://packagist.phpcomposer.com"
        }
    }
}
